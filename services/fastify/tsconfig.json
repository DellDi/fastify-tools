{
  "extends": "fastify-tsconfig",
  "compilerOptions": {
    "outDir": "./api",
    "sourceMap": false,
    "moduleResolution": "NodeNext",
    "allowSyntheticDefaultImports": true,
    "module": "NodeNext",
    "target": "ES2022",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "#/*": ["./src/*"]
    }
  },
  "include": [
    "src/**/*.ts",
  ]
}
