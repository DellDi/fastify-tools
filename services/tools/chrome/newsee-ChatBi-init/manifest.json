{
  "manifest_version": 2,
  "name": "DOM Modifier",
  "version": "1.0",
  "description": "A Chrome extension to modify DOM elements",
  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>"
  ],
  "icons": {
    "48": "ai-logo.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "ai-logo.png"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    "newsee-ChatBI-init.js",
    "content.js"
  ]
}
